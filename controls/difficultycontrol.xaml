<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="Bike.Controls.DifficultyControl" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="1*" />
      <ColumnDefinition Width="1*" />
      <ColumnDefinition Width="1*" />
      <ColumnDefinition Width="1*" />
      <ColumnDefinition Width="1*" />
    </Grid.ColumnDefinitions>
    <Image Name="image" Margin="0,0,0.333,0" Source="{Binding ImageStr[0]}" />
    <Image Name="image_Copy" Grid.Column="1" Source="{Binding ImageStr[1]}" />
    <Image Name="image_Copy1" Grid.Column="2" Margin="0,0,0,0" Source="{Binding ImageStr[2]}" />
    <Image Name="image_Copy2" Grid.Column="3" Source="{Binding ImageStr[3]}" />
    <Image Name="image_Copy3" Grid.Column="4" Margin="0,0,0,0" Source="{Binding ImageStr[4]}" />
  </Grid>
</UserControl>